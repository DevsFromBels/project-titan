{
	"expo": {
		"scheme": "Titan Advertisiment",
		"name": "Titan Advertisiment",
		"slug": "mobile-app",
		"userInterfaceStyle": "automatic",
		"version": "1.0.0",
		"orientation": "portrait",
		"icon": "./assets/logo.png",
		"splash": {
			"image": "./assets/splash-screen.png",
			"resizeMode": "contain",
			"backgroundColor": "#000000"
		},
		"assetBundlePatterns": ["**/*"],
		"ios": {
			"supportsTablet": true,
			"bundleIdentifier": "com.dev1siwoo.mobileapp"
		},
		"android": {
			"adaptiveIcon": {
				"foregroundImage": "./assets/adaptive-icon.png",
				"backgroundColor": "#000000"
			},
			"package": "com.dev1siwoo.mobileapp"
		},
		"web": {
			"favicon": "./assets/logo.png"
		},
		"plugins": ["expo-router", "expo-font", "expo-localization"],
		"extra": {
			"router": {
				"origin": false
			},
			"eas": {
				"projectId": "4ec0e371-6bfe-4988-a11b-60a69299f623"
			}
		}
	}
}
